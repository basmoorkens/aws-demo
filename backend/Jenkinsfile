pipeline {
    agent any
	stages {
	stage ('checkout scm') {
			steps {
				checkout scm
			}
		}
        stage('Build') {
            steps {
		    sh '''
                    echo "PATH = ${PATH}"
                    echo "M2_HOME = ${M2_HOME}"
                '''
               sh 'mvn clean install'
            }
        }
	}
}
