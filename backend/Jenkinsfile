pipeline {
    agent any
	stages {
		stage ('checkout scm') {
			steps {
				checkout scm
			}
		}
        	stage('Build') {
            		steps {
               			sh 'mvn -f backend/pom.xml clean install'
            		}
        	}
		
		stage('Docker build') {
			steps {
		 		sh 'docker build -t 163091829738.dkr.ecr.eu-west-1.amazonaws.com/testje:latest ./backend &&
				docker push'
			}
		}
	}
}
